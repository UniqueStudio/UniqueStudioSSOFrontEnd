    "http-proxy-middleware": "^2.0.6",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "pinia": "^2.1.4",
    "qs": "^6.11.2",
    "vue": "^3.4.27",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@arco-plugins/vite-vue": "^1.4.5",
    "@types/lodash": "^4.14.195",
    "@types/mockjs": "^1.0.7",
