{
  "name": "uniquestudiocandidate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host=0.0.0.0 --config ./config/vite.config.dev.ts",
    "build": "vue-tsc && vite build --config ./config/vite.config.base.ts",
    "preview": "vite preview",
